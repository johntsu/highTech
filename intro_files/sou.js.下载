function init() {
 var p = document.getElementById("course");
 var e = " ";
 if ( p && e ) {
  for( var i = 1; i < pc.length; i++ ) {
   e = document.createElement( "option" );
   e.setAttribute( "value", i );
   e.innerHTML = pc[i][0];
   p.appendChild(e);
  }
  showcity(1);
 }
}

function selectcity() {
 var p = document.getElementById("course");
 var idx = p.options[p.selectedIndex].value;
 if ( idx ) {
  showcity(idx);
 }
}

function showcity(idx) {
 var c = document.getElementById("city");
 if ( c ) {
  while( c.hasChildNodes() ) {
   c.removeChild(c.lastChild);
  }
 }
 if ( idx ) {
  var citys = pc[idx][1].split( "|" ); 
  
  for( var i = 1; i < citys.length; i++ ) {
   e = document.createElement( "option" );
   e.setAttribute( "value", citys[i] );
   e.innerHTML = citys[i];
   c.appendChild(e);
  }
 }
}

