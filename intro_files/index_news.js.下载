var dt_img = $('.dl_list');
dt_img.hover(function () {
    //alert(111);
    $(this).find('dt >img').removeClass('img_hover2').addClass('img_hover1');
},function () {
    $(this).find('dt >img').removeClass('img_hover1').addClass('img_hover2');
});
